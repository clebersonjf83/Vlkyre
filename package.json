{
    "main": "index.js",
    "version": "0.2.3",
    "name": "vlkyre-bot",
    "description": "Vlkyre is a whatsapp user-bot with automation, moderation, music, games and 200+ commands!",
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.22.19"
    },
    "scripts": {
        "test": "supervisor test",
        "ffdev": "nodemon pfmpeg.js",
        "ffstart": "forever pfmpeg.js",
        "dev": "supervisor -w . index.js",
        "local": "node --no-warnings boot.js",
        "magdev": "concurrently --kill-others \"yarn dev\" \"yarn ffdev\"",
        "magstart": "concurrently --kill-others \"yarn start\" \"yarn ffstart\"",
        "start": "forever --minUptime=1000ms --spinSleepTime=1000ms boot.js --color",
        "build": "rm -f yarn.lock && pip3 install -r requirements.txt && yarn install"
    },
    "depecrated": false,
    "directories": {
        "src": "src",
        "bin": "bin",
        "auth": "auth",
        "docs": "docs",
        "views": "views",
        "logger": "logger",
        "models": "models",
        "server": "server",
        "database": "database"
    },
